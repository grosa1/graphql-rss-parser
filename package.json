{
  "name": "graphql-rss-parser",
  "version": "2.6.2",
  "description": "A microservice that parses rss feeds and makes it available as grahpql schema",
  "main": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "graphql-rss-parser": "./cli.js"
  },
  "scripts": {
    "start": "./cli.js",
    "dev": "nodemon ./cli.js",
    "test": "jest",
    "lint": "eslint src *.js",
    "release": "standard-version -i changelog.md -m '%s'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relekang/graphql-rss-parser.git"
  },
  "author": "Rolf Erik Lekang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relekang/graphql-rss-parser/issues"
  },
  "homepage": "https://github.com/relekang/graphql-rss-parser#readme",
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "testRegex": "/__tests__/.*\\.tests\\.js$"
  },
  "dependencies": {
    "@sentry/node": "^5.24.2",
    "apollo-server-micro": "^2.18.1",
    "args": "^3.0.8",
    "axios": "^0.20.0",
    "cheerio": "^1.0.0-rc.3",
    "debug": "^4.2.0",
    "feedme": "^1.2.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.3",
    "http-status-codes": "^2.1.4",
    "is-url": "^1.2.4",
    "lodash-fp": "^0.10.4",
    "micro": "^9.3.4",
    "node-feedparser": "^1.0.1",
    "normalize-url": "^5.2.1",
    "rss-parser": "^3.9.0",
    "rss-to-json": "^1.1.1",
    "update-notifier": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra-promise": "^1.0.1",
    "jest": "^26.4.2",
    "nock": "^13.0.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "standard-version": "^9.0.0"
  },
  "resolutions": {
    "node-fetch": ">=2.6.1 <3.0.0-beta.1|| >= 3.0.0-beta.9",
    "lodash": ">=4.17.19",
    "minimist": ">=0.2.1 <1.0.0 || >=1.2.3",
    "acorn": ">=5.7.4 <6.0.0 || >=6.4.1 <7.0.0 || >=7.1.1"
  }
}
