{
  "name": "micro-rss-parser",
  "version": "2.3.0",
  "description": "A microservice that parses rss feeds and returns JSON",
  "main": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "micro-rss-parser": "./cli.js"
  },
  "scripts": {
    "start": "./cli.js",
    "dev": "nodemon ./cli.js",
    "test": "jest",
    "lint": "eslint src *.js",
    "release": "standard-version -i changelog.md -m '%s'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relekang/micro-rss-parser.git"
  },
  "author": "Rolf Erik Lekang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relekang/micro-rss-parser/issues"
  },
  "homepage": "https://github.com/relekang/micro-rss-parser#readme",
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "testRegex": "/__tests__/.*\\.tests\\.js$"
  },
  "dependencies": {
    "apollo-server-micro": "^2.18.1",
    "args": "^3.0.8",
    "axios": "^0.20.0",
    "cheerio": "^1.0.0-rc.3",
    "feedme": "^1.2.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.3",
    "is-url": "^1.2.4",
    "lodash-fp": "^0.10.4",
    "micro": "^9.3.4",
    "node-feedparser": "^1.0.1",
    "normalize-url": "^5.2.0",
    "raven": "^2.6.4",
    "rss-parser": "^3.9.0",
    "update-notifier": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "fs-extra-promise": "^1.0.1",
    "jest": "^25.1.0",
    "nock": "^11.7.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0"
  }
}
